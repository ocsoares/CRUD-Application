  <div class="view-user p-5">
    <div class="row mb-5">
      <div class="col text-center">
        <h3><%- searchUserByID.username %></h3>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Usuário</th>
              <th scope="col">Email</th>
              <th scope="col">Type</th>
              <th scope="col">Criado em</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row"><%- searchUserByID.username %></th>
              <td><%- searchUserByID.email %></td>
              <td><%- searchUserByID.type %></td>
              <td><%- searchUserByID.created_date %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="row">
        <div class="col">
          <b>Logs</b>
        </div>
      </div>
      <div class="row">
        <div class="col">
          <% if(searchLogsAdminByID.length === 0){ %>
            Não existe logs registrado para essa conta.
          <% }%>
          <% searchLogsAdminByID.forEach((searchLogsAdminByID) => { %> <!-- FORA do <tr> !! << -->
            <%- searchLogsAdminByID.comment %>
            <br>
            <% });%>
        </div>
      </div>
    </div>
  </div>