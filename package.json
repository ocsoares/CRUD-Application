  "main": "src/app.ts",
  "scripts": {
    "tscdir": "tsc -w  --noEmit false --outDir dist",
    "start": "node ./dist/app.js",
    "build": "tsc",
    "nodemon": "nodemon ./dist/app.js",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/config/database.ts migration:generate ./src/database/migration/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/config/database.ts migration:run",
    "madge": "madge --circular --extensions ts ./src/database/entity",
    "watchify": "watchify ./dist/scripts/include-view-post.script.js -o dist/build-scripts/include-view-post.js",
    "watchify2": "watchify ./dist/scripts/login-script-html.js -o dist/build/build2.js"
  },
